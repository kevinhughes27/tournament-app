{
  "name": "admin_next",
  "version": "0.1.0",
  "private": true,
  "homepage": "/admin_next",
  "proxy": {
    "/graphql": {
      "target": "http://no-borders.lvh.me:3000",
      "changeOrigin": true
    },
    "/user_token": {
      "target": "http://no-borders.lvh.me:3000",
      "changeOrigin": true
    }
  },
  "scripts": {
    "start": "concurrently \"yarn compile-relay --watch\" \"PORT=4000 react-app-rewired start\"",
    "build": "yarn compile-relay && react-app-rewired build",
    "compile-relay": "relay-compiler --src ./src --schema ../../app/graphql/schema.graphql --extensions tsx ts",
    "type-check": "tsc --noEmit --pretty",
    "test": "react-app-rewired test --env=jsdom"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.0.0-beta.54",
    "@fortawesome/fontawesome-svg-core": "^1.2.0-14",
    "@fortawesome/free-solid-svg-icons": "^5.1.0-11",
    "@fortawesome/react-fontawesome": "^0.1.0-11",
    "@material-ui/core": "^1.2.3",
    "@material-ui/icons": "^1.1.0",
    "@types/lodash": "^4.14.112",
    "@types/node": "^10.0.4",
    "@types/react": "^16.3.13",
    "@types/react-dnd": "^2.0.36",
    "@types/react-dnd-html5-backend": "^2.1.9",
    "@types/react-dom": "^16.0.5",
    "@types/react-leaflet": "^1.1.5",
    "@types/react-relay": "^1.3.1",
    "@types/react-router": "^4.0.28",
    "@types/react-router-dom": "^4.2.7",
    "@types/relay-runtime": "^1.3.3",
    "babel-plugin-relay": "^1.6.0",
    "concurrently": "^3.6.0",
    "email-validator": "^2.0.4",
    "formik": "^1.0.0-beta.9",
    "jwt-decode": "^2.2.0",
    "leaflet": "^1.3.2",
    "local-storage": "^1.4.2",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "react": "^16.4.0",
    "react-app-rewire-typescript-babel-preset": "^2.3.0",
    "react-app-rewired": "^2.0.0",
    "react-datepicker": "^0.51.0",
    "react-dnd": "^2.4.0",
    "react-dnd-html5-backend": "^2.4.1",
    "react-dom": "^16.4.0",
    "react-join": "^1.1.4",
    "react-leaflet": "^1.9.1",
    "react-relay": "^1.6.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.0-next.3e165448",
    "relay-compiler": "^1.6.0",
    "relay-runtime": "^1.6.0",
    "tslint": "^5.10.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.9.2",
    "utility-types": "^2.0.0"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">0.25%",
      "not op_mini all",
      "ie 11"
    ]
  }
}
