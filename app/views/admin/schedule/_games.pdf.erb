<h4>Games</h4>

<table class="table table-bordered table-striped table-hover table-condensed">
  <thead>
    <tr>
      <th>Game</th>
      <th>Pool</th>
      <th>Home</th>
      <th>Away</th>
      <th>Score</th>
      <th>Notes</th>
    </tr>
  </thead>
  <tbody>
    <% games.each do |game| %>
      <tr>
        <td><%= game.bracket_uid %></td>
        <td><%= game.pool %></td>
        <td><%= game.home.present? ? game.home_name : game.home_prereq_uid %></td>
        <td><%= game.away.present? ? game.away_name : game.away_prereq_uid %></td>
        <td></td>
        <td></td>
      </tr>
    <% end %>
  </tbody>
</table>
