<table class="table table-bordered table-striped table-hover">
  <thead>
    <tr>
      <th>Team</th>
      <th>#</th>
    </tr>
  </thead>
  <tbody>
    <% teams.each do |team| %>
      <tr>
        <td><%= team.name %></td>
        <td><%= team.id %></td>
      </tr>
    <% end %>
  </tbody>
</table>
