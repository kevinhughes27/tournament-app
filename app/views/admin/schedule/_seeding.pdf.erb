<div class="col-xs-2">
  <table class="table table-bordered table-striped table-hover table-condensed">
    <thead>
      <tr>
        <th>Team</th>
        <th>seed</th>
      </tr>
    </thead>
    <tbody>
      <% division.teams.sort_by{ |t| t.seed || 1000 }.each do |team| %>
        <tr>
          <td><%= team.name %></td>
          <td><%= team.seed %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
