<% content_for :top_bar_left do %>
  <i class="fa fa-calendar"></i>
  Schedule
<% end %>

<%= content_for :top_bar_right do %>
  <%= link_to 'View Pdf', admin_schedule_path(format: :pdf), class: 'btn btn-default', target: '_blank' %>
<% end %>

<%= react_component('ScheduleEditor', props: {
  fields: render(template: 'admin/fields/fields.json.jbuilder', locals: {fields: @fields}),
  games: render(template: 'admin/games/games.json.jbuilder', locals: {games: @games})
})%>
